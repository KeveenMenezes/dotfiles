{
    "terminal.integrated.fontFamily": "Cascadia Code",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.tabs.defaultIcon": "github-action",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.suggest.selectionMode": "always",
    "diffEditor.wordWrap": "on",
    "editor.defaultFormatter": "ms-dotnettools.csdevkit",
    "editor.linkedEditing": true,
    "editor.fontFamily": "Cascadia Code",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": "active",
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 120,
    "editor.rulers": [100],
    "editor.formatOnSave": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showStatusBar": true,
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "editor.minimap.renderCharacters": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.ignoreRebaseWarning": true,
    "git.enableSmartCommit": true,
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "tailwindCSS.suggestions": true,
        "editor.tabCompletion": "on"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "tailwindCSS.suggestions": true,
        "editor.tabCompletion": "on"
    },
    "[html]": {
        "editor.defaultFormatter": "bradlc.vscode-tailwindcss",
        "tailwindCSS.suggestions": true,
        "editor.tabCompletion": "on"
    },
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "[proto3]": {
        "editor.defaultFormatter": "zxh404.vscode-proto3",
        "editor.tabCompletion": "on"
    },
    "tailwindCSS.includeLanguages": {
        "plaintext": "html"
    },
    "prettier.singleAttributePerLine": true,
    "prettier.tabWidth": 4,
    "prettier.vueIndentScriptAndStyle": true,
    "powermode.explosions.frequency": 1,
    "powermode.enabled": true,
    "powermode.shake.intensity": 0,
    "powermode.presets": "flames",
    "powermode.combo.location": "off",
    "powermode.combo.counterEnabled": "hide",
    //Vim
    "vim.autoindent": false,
    "vim.camelCaseMotion.enable": true,
    "vim.leader": "<space>",
    "vim.smartRelativeLine": true,
    "vim.hlsearch": true,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.normalModeKeyBindings": [
        // NAVIGATION
        // switch b/w buffers
        {
            "before": ["<S-h>"],
            "commands": [":bprevious"]
        },
        {
            "before": ["<S-l>"],
            "commands": [":bnext"]
        },
        {
            "before": ["leader", "leader", "leader", "y"],
            "commands": [
                { "command": "workbench.action.closeAuxiliaryBar" },
                { "command": "workbench.action.closeSidebar" },
                { "command": "workbench.action.closePanel" }
            ]
        },
        {
            "before": ["leader", "c"],
            "commands": [{ "command": "workbench.action.toggleCenteredLayout" }]
        },
        // panes
        {
            "before": ["leader", "h"],
            "commands": ["workbench.action.focusLeftGroup"]
        },
        {
            "before": ["leader", "j"],
            "commands": ["workbench.action.focusBelowGroup"]
        },
        {
            "before": ["leader", "k"],
            "commands": ["workbench.action.focusAboveGroup"]
        },
        {
            "before": ["leader", "l"],
            "commands": ["workbench.action.focusRightGroup"]
        },
        {
            "before": ["leader", "<S-h>"],
            "commands": ["workbench.action.moveEditorToLeftGroup"]
        },
        {
            "before": ["leader", "<S-j>"],
            "commands": ["workbench.action.moveEditorToBelowGroup"]
        },
        {
            "before": ["leader", "<S-k>"],
            "commands": ["workbench.action.moveEditorToAboveGroup"]
        },
        {
            "before": ["leader", "<S-l>"],
            "commands": ["workbench.action.moveEditorToRightGroup"]
        },
        {
            "before": ["<leader>", "m"],
            "commands": ["bookmarks.toggle"]
        },
        {
            "before": ["<leader>", "<S-m>"],
            "after": [
                "o",
                "/",
                "/",
                "T",
                "O",
                "D",
                "O",
                ":",
                " ",
                "<esc>",
                "space",
                "m",
                "a"
            ]
        },
        {
            "before": ["<leader>", "b"],
            "commands": ["bookmarks.list"]
        },
        {
            "before": ["<leader>", "r"],
            "after": [":", "%", "s", "/"]
        },
        {
            "before": ["<leader>", "<leader>", "d"], //run docker compose
            "commands": ["editor.debug.action.runToCursor"]
        },
        {
            "before": ["<leader>", "<leader>", "r"], //run file http
            "commands": ["rest-client.request"]
        },
        {
            "before": ["<leader>", "n"],
            "commands": ["workbench.action.moveEditorToNewWindow"],
            "when": ["activeEditor"]
        },
        {
            "before": ["<C-d>"],
            "after": ["<C-d>", "z", "z"]
        },
        {
            "before": ["<C-u>"],
            "after": ["<C-u>", "z", "z"]
        },
        {
            "before": ["n"],
            "after": ["n", "z", "z", "z", "v"]
        },
        {
            "before": ["N"],
            "after": ["N", "z", "z", "z", "v"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["J"],
            "after": [
                ":",
                "m",
                " ",
                "'",
                ">",
                "+",
                "1",
                "<CR>",
                "g",
                "v",
                "=",
                "g",
                "v"
            ]
        },
        {
            "before": ["K"],
            "after": [
                ":",
                "m",
                " ",
                "'",
                "<",
                "-",
                "2",
                "<CR>",
                "g",
                "v",
                "=",
                "g",
                "v"
            ]
        }
    ],
